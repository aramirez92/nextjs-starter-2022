{
  "name": "nextjs-starter-2021",
  "private": true,
  "scripts": {
    "build": "next build",
    "commit": "cz",
    "dev": "next dev",
    "lint": "eslint . --ext .ts,.tsx",
    "prepare": "husky install",
    "start": "next start"
  },
  "dependencies": {
    "next": "12.0.4",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "15.0.0",
    "@commitlint/config-conventional": "15.0.0",
    "@types/node": "16.11.8",
    "@types/react": "17.0.35",
    "@typescript-eslint/eslint-plugin": "5.4.0",
    "@typescript-eslint/parser": "5.4.0",
    "commitizen": "4.2.4",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.2.0",
    "eslint-config-next": "12.0.4",
    "eslint-plugin-react": "7.27.1",
    "husky": "7.0.4",
    "lint-staged": "12.0.3",
    "typescript": "4.5.2"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": ["eslint"]
  }
}
