{
  "name": "nextjs-starter-2021",
  "version": "1.0.1",
  "private": true,
  "scripts": {
    "build": "next build",
    "commit": "cz",
    "dev": "next dev",
    "lint": "eslint . --ext .ts,.tsx",
    "prepare": "husky install",
    "semantic-release": "semantic-release",
    "start": "next start",
    "storybook": "start-storybook -p 6006 -s ./public",
    "build-storybook": "build-storybook -s public"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "1.2.36",
    "@fortawesome/free-brands-svg-icons": "5.15.4",
    "@fortawesome/free-solid-svg-icons": "5.15.4",
    "@fortawesome/react-fontawesome": "0.1.16",
    "next": "12.0.4",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "styled-components": "5.3.3",
    "twin.macro": "2.8.1"
  },
  "devDependencies": {
    "@babel/core": "7.16.0",
    "@commitlint/cli": "15.0.0",
    "@commitlint/config-conventional": "15.0.0",
    "@headlessui/react": "1.4.2",
    "@heroicons/react": "1.0.5",
    "@semantic-release/changelog": "6.0.1",
    "@semantic-release/git": "10.0.1",
    "@storybook/addon-actions": "6.4.4",
    "@storybook/addon-essentials": "6.4.4",
    "@storybook/addon-links": "6.4.4",
    "@storybook/addon-postcss": "2.0.0",
    "@storybook/builder-webpack5": "6.4.4",
    "@storybook/manager-webpack5": "6.4.4",
    "@storybook/react": "6.4.4",
    "@types/node": "16.11.8",
    "@types/react": "17.0.35",
    "@types/styled-components": "5.1.15",
    "@typescript-eslint/eslint-plugin": "5.4.0",
    "@typescript-eslint/parser": "5.4.0",
    "autoprefixer": "10.4.0",
    "babel-loader": "8.2.3",
    "babel-plugin-macros": "3.1.0",
    "babel-plugin-styled-components": "2.0.0",
    "commitizen": "4.2.4",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.2.0",
    "eslint-config-next": "12.0.4",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.27.1",
    "husky": "7.0.4",
    "lint-staged": "12.0.3",
    "postcss": "8.3.11",
    "prettier": "2.4.1",
    "semantic-release": "18.0.0",
    "stylelint": "14.1.0",
    "stylelint-config-standard": "24.0.0",
    "tailwindcss": "2.2.19",
    "typescript": "4.5.2"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint"
    ]
  },
  "babelMacros": {
    "twin": {
      "preset": "styled-components"
    }
  }
}
